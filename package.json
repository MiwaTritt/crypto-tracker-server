{
  "name": "crypto-tracker-server",
  "version": "1.0.0",
  "main": "dist",
  "repository": "https://github.com/TaraTritt/crypto-tracker-server.git",
  "author": "TaraTritt <tmt7rc@virginia.edu>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets env\"",
    "build": "babel src -s -D -d dist --presets env",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "mocha --require babel-register \"./src/**/*.spec.js\"",
    "test-all": "mocha --require babel-register ./src/test/**/*.js"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "mongoose": "^5.0.16",
    "morgan": "^1.8.0",
    "node-fetch": "^2.1.2",
    "resource-router-middleware": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.17.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.9.2",
    "sinon": "^4.5.0"
  }
}
